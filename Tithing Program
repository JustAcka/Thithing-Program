using namespace std;

#include <iostream>

int main() {
    
    //Delcaired Variables
    double moneyEarned = 0.0;
    const double TITHING_PERCENT = .10;
    
    //User Imput
    cout << "Enter Amount Earned: ";
    cin >> moneyEarned;
    
    //Imput Output
    double tithingAmount = moneyEarned * TITHING_PERCENT;
    
    //User Display
    
    cout << "Tithing Amount: $" << tithingAmount << endl;
    
    return 0;
}
